<%- include("./includes/head.ejs") %>

<link rel="stylesheet" href="./css/login.css">

<div class="m-top">
  <a href="#" class="m-login-head">TheHanger</a>
  <span style="font-size: 14px;">(Business)</span>
</div>
<div class="container-fluid">
    <div class="row justify-content-between m-m-login" style="margin-top: auto;">
        <div class="col-4">
            
        </div>
        <div class="col-3 m-d-login mt-4 pt-5" style="padding:0px">
          <form action="/auth/corporate-signUp" method="POST" class="my-4 p-2 text-center mt-4">
            <div class="form-group">
              <label for="email">Corporate Email address</label> <br>
              <input type="email" name="email" class="m-inp">
              <small id="emailHelp" class="form-text">We'll never share your email with anyone else.</small>
            </div>
            <div class="form-group">
              <label>Corporate Token</label> <br>
              <input type="text" name="token" class="m-inp">
            </div>
            <div class="form-group">
              <label>Password</label> <br>
              <input type="password" name="password" class="m-inp">
            </div>
            <div class="form-group">
              <label>Confirm Password</label> <br>
              <input type="password" name="c-password" class="m-inp">
            </div>
            <button type="submit" class="btn btn-outline-light">Sign Up</button>
          </form>
        </div>
        <div class="col-4">

        </div>
    </div>
</div>